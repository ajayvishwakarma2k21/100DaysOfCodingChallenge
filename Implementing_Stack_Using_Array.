class ArrayStack {
    private int[] stack;
    private int top;

    public ArrayStack() {
        stack = new int[1000];
        top = -1;
    }

    public void push(int x) {
        if(top < 1000 - 1){
            stack[++top] = x;
        }
    }

    public int pop() {
        if(isEmpty())return -1;
      return stack[top--];
    }

    public int top() {
        if(isEmpty())return -1;
        return stack[top];
    }

    public boolean isEmpty() {
        if(top == -1)return true;
        return false;
    }
}
