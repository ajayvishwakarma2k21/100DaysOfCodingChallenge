class Solution {
    public boolean isValid(String s) {
      if(s.isEmpty()|| s.length()== 1)return false;
        Stack<Character> st = new Stack<>();
        for(char c : s.toCharArray()){
          if(c == '('|| c == '{' || c == '['){
            st.push(c);
          }else{
            if(st.isEmpty())return false;
            char t = st.pop();
            if((t == '(' && c == ')') || (t == '{' && c == '}') || (t == '[' && c == ']')){
              continue;
            }else{
              return false;
            }
          }
        }
        return st.isEmpty();
    }
}
